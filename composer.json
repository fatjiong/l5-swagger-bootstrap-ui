{
    "name": "x-class/l5-swagger-bootstrap-ui",
    "description": "Swagger UI to Laravel 5",
    "keywords": [
        "laravel",
        "swagger",
        "api",
        "bootstrap",
        "UI"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mr.Awei",
            "email": "515242609@qq.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "5.5.*|5.6.*|5.7.*",
        "darkaonline/l5-swagger": "5.5.*|5.6.*|5.7.*"
    },
    "require-dev": {
    },
    "suggest": {
        "zircote/swagger-php:~3.0": "!!! Require Swagger-PHP ~3.0 for @OA annotations support !!!"
    },
    "autoload": {
        "psr-4": {
            "L5SwaggerBootStrapUi\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
    },
    "extra": {
        "laravel": {
            "providers": [
                "L5SwaggerBootStrapUi\\L5SwaggerBootStrapUiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
